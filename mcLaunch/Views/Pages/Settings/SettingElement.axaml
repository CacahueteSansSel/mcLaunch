<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="36" Height="36"
             x:Class="mcLaunch.Views.Pages.Settings.SettingElement">
    <UserControl.Styles>
        <StyleInclude Source="/Styles/AppStyles.axaml" />
    </UserControl.Styles>
    <DockPanel VerticalAlignment="Center">
        <TextBlock x:Name="Label" VerticalAlignment="Center" Classes="light" Margin="15 0 0 0">hello</TextBlock>
        <Panel x:Name="Contents" DockPanel.Dock="Right" Margin="0 0 5 0">
            <ToggleSwitch x:Name="BooleanCheckbox" OffContent="" OnContent="" IsCheckedChanged="BooleanCheckboxChecked" />
        </Panel>
        <Panel />
    </DockPanel>
</UserControl>